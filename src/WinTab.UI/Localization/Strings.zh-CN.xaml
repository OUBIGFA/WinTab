    <!-- Auto Grouping Page -->
    <system:String x:Key="AutoGroup_Title">自动分组规则</system:String>
    <system:String x:Key="AutoGroup_Description">当新窗口出现时，自动根据规则将其添加到对应的分组。</system:String>
    <system:String x:Key="AutoGroup_AddRule">添加规则</system:String>
    <system:String x:Key="AutoGroup_RemoveRule">移除规则</system:String>
